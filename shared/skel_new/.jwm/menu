        	<Program label="Terminal">lxterminal</Program>
        	<Program label="File Manager">pcmanfm</Program>
        	<Program label="Web Browser">palemoon</Program>
		<Separator/>
		<Menu label="Applications">
			<Include>exec: xdgmenumaker -n -f jwm</Include>
		</Menu>
		<Menu label="CLI Utilities">			
                        <Program label="WelcomeLive">lxterminal --geometry=130x29 --title=WelcomeLive --command=/usr/bin/kibojoe/welcome</Program>                        
			<Program label="BFUSBTool">lxterminal --geometry=77x16 --title=BFUSBTool --command=/usr/bin/kibojoe/bfusb</Program>
	        	<Program label="FLocate">lxterminal --geometry=77x22 --title=FLocate --command=/bin/kibojoe/flocate</Program>
			<Program label="IS">lxterminal --geometry=77x27 --title=IS --command=/usr/bin/kibojoe/is</Program>
			<Program label="JWMConf">lxterminal --geometry=77x27 --title=JWMConf --command=/usr/bin/kibojoe/jwmconf</Program>        	
                	<Program label="KDUSM">lxterminal --geometry=77x21 --title=KDUSM --command=/usr/bin/kibojoe/kdusm</Program>		
                	<Program label="KJWMPostI">lxterminal --geometry=80x23 --title=KJWMPostI --command=/usr/bin/kibojoe/kjwmpost</Program>
                	<Program label="NMTUI">lxterminal --geometry=77x25 --title=NMTUI --command=nmtui</Program>
			<Program label="Pacli-SysClean">lxterminal --geometry=77x25 --title=Pacli-SysClean --command=/usr/bin/kibojoe/pacli</Program>                     	
