	<!-- Ctrl (Control) key. -->
    <Key mask="C" key="l">exec:/usr/bin/kibojoe/leave_jwm</Key>

    <!-- Key sound. -->
	<!-- Windows (Super) key. -->
    <Key mask="4" key="Next">exec:/usr/bin/pulseaudio-ctl down 5</Key>
    <Key mask="4" key="Prior">exec:/usr/bin/pulseaudio-ctl up 5</Key>
    <Key mask="4" key="0">exec:/usr/bin/pulseaudio-ctl mute</Key>
    
    <!-- Key monitor brightness. -->
	<!-- Ctrl (Control) key. -->
    <Key mask="C" key="Next">exec:light -U 5; notify-send 'Brightness down 5 %'</Key>
    <Key mask="C" key="Prior">exec:light -A 5; notify-send 'Brightness up 5 %'</Key>

    <!-- Key toggle. -->
	<!-- Windows (Super) key. -->
    <Key mask="4" key="Up">maxtop</Key>
    <Key mask="4" key="Down">maxbottom</Key>
    <Key mask="4" key="Left">maxleft</Key>
    <Key mask="4" key="Right">maxright</Key>

    <!-- Key general. -->
	<!-- Windows (Super) key. -->
    <Key mask="4" key="g">exec:epdfview /usr/share/doc/kibojoe/User_Orientation_Guide.pdf</Key>
	<!-- Alone key. -->
    <Key key="Print">exec:scrot -e 'mv $f ~/Downloads' &amp;&amp; sleep 1 &amp;&amp; notify-send 'Screenshot has been saved in ~/Downloads'</Key>
    
