    <Key mask="4" key="d">showdesktop</Key>
    <Key mask="4" key="m">exec:deadbeef</Key>
    
    <!-- Key system. -->
	<!-- Windows (Super) key. -->
    <Key mask="4" key="F1">root:3</Key>
    <Key mask="4" key="c">exec:sakura -c 80 -r 22 -T FLocate -e "bash -c '/usr/bin/kibojoe/flocate;exec bash'"</Key>
    <Key mask="4" key="p">exec:sakura -c 80 -r 25 -T Pacli-SysClean -e "bash -c '/usr/bin/kibojoe/pacli;exec bash'"</Key>
    <Key mask="4" key="j">exec:sakura -c 80 -r 27 -T JWMConf -e "bash -c '/usr/bin/kibojoe/jwmconf;exec bash'"</Key>
